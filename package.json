{
  "name": "nation-gate-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:server": "SET NODE_ENV=production&&node server.js",
    "lint": "next lint",
    "migration:generate": "drizzle-kit generate:pg",
    "migration:push": "node -r esbuild-register src/db/migrate.ts",
    "db:seed": "node -r esbuild-register src/db/seed.ts ",
    "prisma:generate": "npx dotenv -e .env.local -- prisma generate",
    "prisma:migrate:dev": "npx dotenv -e .env.local -- prisma migrate dev",
    "prisma:migrate:reset": "npx dotenv -e .env.local -- prisma migrate reset",
    "prisma:db:seed": "npx dotenv -e .env.local -- prisma db seed",
    "prisma:studio": "npx dotenv -e .env.local -- prisma studio",
    "prisma:migrate:deploy": "npx dotenv -e .env.local -- prisma migrate deploy"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.12.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-query": "^5.27.5",
    "@tanstack/react-table": "^8.13.2",
    "axios": "^1.6.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "date-fns": "^3.4.0",
    "drizzle-orm": "^0.30.2",
    "lucide-react": "^0.356.0",
    "next": "14.1.3",
    "next-pwa": "^5.6.0",
    "node-windows": "^1.0.0-beta.8",
    "pg": "^8.11.3",
    "puppeteer": "^22.6.1",
    "react": "^18",
    "react-barcode": "^1.5.1",
    "react-dom": "^18",
    "react-hook-form": "^7.51.2",
    "react-to-print": "^2.15.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zebra-browser-print-wrapper": "^0.1.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.27.7",
    "@types/node": "^20",
    "@types/pg": "^8.11.2",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "dotenv-cli": "^7.4.1",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "postcss": "^8",
    "prisma": "^5.12.1",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
